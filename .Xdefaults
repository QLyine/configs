! .Xdefaults

! Xcursor --------------------------------------------------------------------

Xcursor.theme: jimmac

! Xft font settings -----------------------------------------------------------

Xft.dpi:        102
Xft.antialias:  1
Xft.hinting:    1
Xft.autohint:   0
Xft.hintstyle:  hintfull
!Xft.hintstyle:  hintslisght
Xft.lcdfilter:  lcddefault
Xft.rgba:       rgb 
 

! URxvt settings --------------------------------------------------------------
!URxvt.termName:         rxvt-256color
URxvt.termName:         rxvt-unicode-256color
URxvt.scrollTtyOutput:  false
URxvt.scrollTtyKeypress:false
URxvt.loginShell:       true
!URxvt.cursorColor:      #B03060
URxvt.colorUL:          #84B5C1
URxvt.pointerBlank:     true
URxvt.visualBell:       false 
URxvt.fading:           10
URxvt.scrollBar:        false
URxvt.saveLines:        10000
URxvt.tintColor:        grey80
!urxvt*geometry:         84x26
URxvt.depth:            32

! fonts 

!urxvt.font:xft:Dina:pixelsize=10
!urxvt.boldFont:xft:Dina:pixelsize=10

!urxvt.font:xft:Dina:pixelsize=13:antialias=true:autohint=true:hinting=true:dpi=128
!urxvt.boldFont:xft:Dina:pixelsize=13:antialias=true:autohint=true:hinting=true:dpi=128

!urxvt.font:xft:Inconsolata:Medium:pixelsize=11
!urxvt.boldFont:xft:Inconsolata:Medium:pixelsize=11

!urxvt.font:xft:Monaco:Regular:pixelsize=11
!urxvt.boldFont:xft:Monaco:Regular:pixelsize=11

URxvt.font:xft:Envy Code R:Regular:pixelsize=11
URxvt.boldFont:xft:Envy Code R:Regular:pixelsize=11

!urxvt.font:xft:Anonymous Pro:Regular:pixelsize=12
!urxvt.boldFont:xft:Anonymous Pro:Regular:pixelsize=12

!urxvt.font:xft:Droid Sans:Regular:pixelsize=10
!urxvt.boldFont:xft:Droid Sans Mono:Regular:pixelsize=10

!urxvt.font:xft:MonteCarlo:Sans:pixelsize=11
!urxvt.boldFont:xft:MonteCarlo:Sans:pixelsize=11

!urxvt.font:xft:Terminus:pixelsize=13:antialias=true:autohint=true:hinting=true:dpi=128
!urxvt.font:xft:Liberation Sans:pixelsize=15:antialias=true:autohint=true:hinting=true:dpi=128
!urxvt.font:xft:Profont:pixelsize=15:antialias=true:autohint=true:hinting=true:dpi=128
!urxvt.font:xft:Monaco:pixelsize=12:antialias=true:autohint=true:hinting=true:dpi=128

! perl
URxvt.perl-lib: /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,matcher,searchable-scrollback
 
! url launchable
URxvt.urlLauncher: /usr/bin/firefox
URxvt.matcher.button: 1

URxvt.matcher.pattern.1: \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt.keysym.C-u: perl:matcher
URxvt.cutchars: "()*,<>[]{}|'

! terminal colors ------------------------------------------------------------

! force default BG & FG
!URxvt.background:    [90]#0c0c0c
!URxvt.foreground:	#ffffff

!#include "/home/qlyine/.colorscheme/nu"
!#include "/home/qlyine/.colorscheme/molokai_v2"
!#include "/home/qlyine/.colorscheme/orange"
#include "/home/qlyine/.colorscheme/marzo"
!#include "/home/qlyine/.colorscheme/cloudef"
!#include "/home/qlyine/.colorscheme/reach"
!#include "/home/qlyine/.colorscheme/sicp"
!#include "/home/qlyine/.colorscheme/justin"
!#include "/home/qlyine/.colorscheme/arthur"
!#include "/home/qlyine/.colorscheme/warm"
!#include "/home/qlyine/.colorscheme/neon"
!#include "/home/qlyine/.colorscheme/getfresh"
!#include "/home/qlyine/.colorscheme/samtole40"
!#include "/home/qlyine/.colorscheme/zenburn"

! force default BG & FG
!*background:    [90]#0c0c0c
!*foreground:	#ffffff


! Xterm Settings
xterm*faceName: Bitstream Vera Sans Mono
xterm*faceSize: 10
xterm*eightBitInput: false
XTerm*metaSendsEscape: true
xterm*boldMode: off
xterm*scrollBar: false
xterm*saveLines: 1000
xterm*Foreground: #cccccc
xterm*Background: #0d0d0d
xterm*Border: #303430
xterm*TitleBar: on
xterm*TextUnderIcon: on
xterm*AllowIconInput: on
xterm*AutoRaise: on
xterm*termtype: vt100
xterm*ttyModes: intr ^c erase ^h susp ^@ dsusp ^@ kill ^u eof ^d
xterm*jumpScroll: true
xterm*multiScroll: true
xterm*backarrowKey: false
XTerm*cutNewline: False
XTerm*loginShell: true


